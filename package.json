{
  "name": "color-geyser",
  "displayName": "Color Geyser",
  "description": "TBD",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:color-geyser.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "color-geyser.helloWorld",
